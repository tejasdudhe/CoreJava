package day3_task;

public class Account {

	int accNumber;
	String custName;
	double balance;
	String city;

	static int genId;

	static {
		genId = 1001;
	}

		public Account(int accNumber, String custName, double balance, String city) {
		this.accNumber = accNumber;
		this.custName = custName;
		this.balance = balance;
		this.city = city;
	}

}
