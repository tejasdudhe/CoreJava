package day2_1;

public class CustomerMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class Customer{
	
	int customerId;
	String custname;
	String city;
	
	
	public Customer()
	{
		customerId = 101;
		custname = "Akash";
		city = "Mumbai";
	}
	
	public Customer(int id,String cName, String cty)
	{
		customerId = id;
		custname = cName;
		city = cty;
		
	}
}
