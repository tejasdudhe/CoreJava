package day5_HW;

public class FootBallPlayer extends Player {
	String position;
	int goalsScored;

	public FootBallPlayer() {
		super();

	}

	public FootBallPlayer(String name, int age, String team, String position, int goalsScored) {
		super(name, age, team);
		this.position = position;
		this.goalsScored = goalsScored;

	}

}
